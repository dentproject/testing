#
# ONL format of dent interfaces
#
# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

source /etc/network/interfaces.d/*.intf

# The loopback network interface
auto lo
iface lo inet loopback
    address 20.20.0.1/32

# The management interface
auto ma1
iface ma1 inet dhcp

# connected to IXIA/Keysight
auto swp1
iface swp1 inet static
alias IXIA LINK
    address 20.20.25.2/30

auto swp2
iface swp2 inet static
alias IXIA LINK
    address 20.20.26.2/30

auto swp3
iface swp3 inet static
alias IXIA LINK
    address 20.20.27.2/30

auto swp4
iface swp4 inet static
alias IXIA LINK
    address 20.20.28.2/30


# connected to AGGregators
auto swp47
iface swp47 inet static
alias AGG1 SWP25
    address 20.20.128.1/30

auto swp48
iface swp48 inet static
alias AGG1 SWP26
    address 20.20.129.1/30

auto swp49
iface swp49 inet static
alias AGG2 SWP27
    address 20.20.130.1/30

auto swp50
iface swp50 inet static
alias AGG2 SWP28
    address 20.20.131.1/30

auto swp51
iface swp51 inet static
alias AGG3 SWP29
    address 20.20.132.1/30

auto swp52
iface swp52 inet static
alias AGG3 SWP30
    address 20.20.133.1/30

